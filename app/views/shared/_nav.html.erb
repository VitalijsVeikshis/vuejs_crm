<% if current_staff %>
  <%= link_to 'Log out', destroy_staff_session_path, method: 'delete' %>
<% end %>
<% if current_client %>
  <%= link_to 'Log out', destroy_client_session_path, method: 'delete' %>
<% end %>
<% if !user_signed_in? %>
  <%= link_to 'Log in as staff', new_staff_session_path %> |
  <%= link_to 'Log in as client', new_client_session_path %>
<% end %>